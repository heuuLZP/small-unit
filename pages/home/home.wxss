/* home.wxss */
.header {
  width: 100%;
  height: 100rpx;
  display: flex;
  justify-content:center;
  align-items: center;
  border-top: 2rpx solid #99cc33;
}
.home-wrap {
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content:space-between;
  box-sizing: border-box;
  border-top: 2rpx solid #99cc33;
}
.card {
  width: 33.3%;
  height: 200rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
  border-right: 2rpx solid #99cc33;
  border-bottom: 2rpx solid #99cc33;
  box-sizing: border-box;
}
.card:nth-child(3) {
  border-right: 0;
}